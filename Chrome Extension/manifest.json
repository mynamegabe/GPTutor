{
  "name": "GPTutor Extension",
  "author": "BigBadWolf",
  "description": "Revolutionize your education with us - the ultimate AI-powered study partner.",
  "version": "1.0",
  "homepage_url": "https://github.com/mynamegabe/NUS-Hack-Roll",
  "manifest_version": 3,
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": ["scripting",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/16x16.png",
      "32": "/images/32x32.png",
      "64": "/images/64x64.png",
      "128": "/images/128x128.png"
    }
  },
  "icons": {
    "16": "/images/16x16.png",
    "32": "/images/32x32.png",
    "64": "/images/64x64.png",
    "128": "/images/128x128.png"
  }
}